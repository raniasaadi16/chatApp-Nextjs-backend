{   
    "version": 2,
    "builds": [
        {
          "src": "index.js",
          "use": "@vercel/node"
        }
    ],
    "routes": [{
            "src": "/(.*)",
            "dest": "index.js",
            "headers": { "cache-control": "no-cache, no-store, must-revalidate", "Access-Control-Allow-Origin": "*" }
        }
    ]
}